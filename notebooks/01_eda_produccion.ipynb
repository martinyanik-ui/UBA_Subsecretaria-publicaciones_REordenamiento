# Carga de datos simulados
def cargar_datos_simulados():
    """Cargar datos de ejemplo para desarrollo"""
    fechas = pd.date_range(start='2023-01-01', end='2024-01-01', freq='D')
    
    data = {
        'fecha': np.random.choice(fechas, 500),
        'id_trabajo': [f'TRB-{i:04d}' for i in range(1, 501)],
        'tipo_trabajo': np.random.choice(['Libro', 'Cuadernillo', 'Folleto', 'Tesis', 'Certificado'], 500),
        'departamento': np.random.choice(['Filosof√≠a', 'Letras', 'Historia', 'Antropolog√≠a', 'Biblioteca'], 500),
        'cantidad_paginas': np.random.randint(10, 500, 500),
        'cantidad_copias': np.random.randint(1, 100, 500),
        'estado': np.random.choice(['Pendiente', 'En proceso', 'Completado', 'Entregado'], 500, p=[0.1, 0.3, 0.4, 0.2]),
        'prioridad': np.random.choice(['Alta', 'Media', 'Baja'], 500, p=[0.2, 0.5, 0.3]),
        'tiempo_produccion_horas': np.random.uniform(1, 120, 500),
        'material_principal': np.random.choice(['Papel A4', 'Papel A3', 'Cartulina', 'Pergamino'], 500),
        'costo_estimado': np.random.uniform(100, 10000, 500)
    }
    
    return pd.DataFrame(data)

